{
  "transform": {
    "^.+\\.jsx?$": "babel-jest"
  },
  "coverageDirectory": "./coverage/",
  "collectCoverage": true,
  "collectCoverageFrom": ["src/**/*.js"],
  "coverageThreshold": {
    "./src/extest.js": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  }
}
