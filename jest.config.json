{
  "transform": {
    "^.+\\.jsx?$": "babel-jest"
  },
  "coverageDirectory": "./coverage/",
  "collectCoverage": true,
  "collectCoverageFrom": ["src/extest.js"],
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  }
}
