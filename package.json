{
  "name": "ccms",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "livereload": "livereload ./src/public/",
    "scss:watch": "node-sass --watch ./src/public/stylesheets/src -o ./src/public/stylesheets/dist",
    "cm": "git-cz",
    "test": "jest",
    "dev:watch": "cross-env NODE_ENV=development nodemon --exec babel-node ./src/bin/www.js",
    "dev": "concurrently \"npm run dev:watch\" \"npm run scss:watch\" \"npm run livereload\"",
    "clean": "rimraf ./dist",
    "transpile": "babel ./src --out-dir ./dist",
    "build": "npm run clean && npm run transpile && cpx \"./src/views/**/*\" ./dist/views --clean",
    "format": "prettier src/**/*.js --write",
    "lint": "eslint src/**/*.js --fix"
  },
  "dependencies": {
    "@babel/polyfill": "^7.11.5",
    "cookie-parser": "~1.4.4",
    "cross-env": "^7.0.2",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/eslint-parser": "^7.11.5",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "babel-jest": "^26.5.2",
    "concurrently": "^5.3.0",
    "cpx": "^1.5.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "husky": "^4.3.0",
    "jest": "^26.5.2",
    "livereload": "^0.9.1",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
